{
  "source": {
    "include": [
      "DormRoom.js",
      "Apartment.js",
      "index.js",
      "MaintenanceRequest.js",
      "Student.js",
      "UniversityResidenceSystem.js"
    ],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
